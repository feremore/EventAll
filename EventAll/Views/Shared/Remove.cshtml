<h1>@ViewBag.title</h1>
<form method="post">

    @foreach (var obj in ViewBag.objs)
    {
        <input type="checkbox" name="objIds" id="@obj.ID" value="@obj.ID" />
        <label for="@obj.ID">@obj.Name</label>
        <br />
    }

    <input type="submit" value="Remove Selected @ViewBag.objName" />
</form>