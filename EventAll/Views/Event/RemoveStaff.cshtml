<h1>@ViewBag.title</h1>
<form method="post">

    @foreach (var obj in ViewBag.objs)
    {
        <input type="checkbox" name="objIds" id="@obj.StaffID" value="@obj.StaffID" />
        <input type="hidden" name="EventID" value="@obj.EventID" />
        <label for="@obj.StaffID">@obj.Staff.Name</label>
        <br />
    }

    <input type="submit" value="Remove Selected @ViewBag.objName" />
</form>