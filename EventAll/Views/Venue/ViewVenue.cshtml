    <div class="row" style="margin-bottom:15px;">
        <h1>@Model.Venue.Name:</h1>
        <div class="col-6">
            <table class="table">
                <tr>
                    <td>Capacity</td>
                    <td>Price</td>
                </tr>
                <tr>

                    <td>@Model.Venue.Capacity</td>
