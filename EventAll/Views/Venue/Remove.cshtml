<h1>@ViewBag.title</h1>
<form method="post">

    @foreach (var venue in ViewBag.venues)
    {
        <input type="checkbox" name="venueIds" id="@venue.ID" value="@venue.ID" />
        <label for="@venue.ID">@venue.Name</label>
        <br />
    }

    <input type="submit" value="Remove Selected Venue(s)" />
</form>